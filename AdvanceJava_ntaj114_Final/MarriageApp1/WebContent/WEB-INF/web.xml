<web-app >
  <display-name>MarriageApp</display-name>
  <servlet>
    <servlet-name>check</servlet-name>
    <servlet-class>com.nt.servlet.CheckMarriageEligibilityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>check</servlet-name>
     <url-pattern>/marriageurl</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.nt.servlet.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>test</servlet-name>
     <url-pattern>/testurl</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
     <welcome-file>input.html</welcome-file>
      <welcome-file>input1.html</welcome-file>
      <welcome-file>abc.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>